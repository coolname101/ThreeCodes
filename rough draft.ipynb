    "\t\ttotal = str(eval(problem))\n",
    "\n",
    "\t\tequation.set(total)\n",
    "\n",
    "\t\t# initialize the problem variable\n",
    "\t\t# by empty string\n",
    "\t\tproblem = \"\"\n",
    "\n",
